{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "y/prisma-service/(.*)": "<rootDir>/../libs/prisma-service/src/$1",
    "y/prisma-service": "<rootDir>/../libs/prisma-service/src",
    "@app/jwt/(.*)": "<rootDir>/../libs/jwt/src/$1",
    "@app/jwt": "<rootDir>/../libs/jwt/src",
    "@app/authlib/(.*)": "<rootDir>/../libs/authlib/src/$1",
    "@app/authlib": "<rootDir>/../libs/authlib/src"
  }
}